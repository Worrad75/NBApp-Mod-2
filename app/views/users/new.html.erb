<h1>Sign Up Page</h1>


<% if flash[:messages] %>
    <% flash[:messages].each do |m| %>
        <%=m%>
    <% end %>
<% end %>


<%= form_for @user do |f| %>
    Username: <br>
    <%= f.text_field :username %> <br>
    Password: <br>
    <%= f.password_field :password %> <br>
    <br> 
    <%= f.submit "Create New User" %>
<% end %>
<br><br><br>
Already have an account? Sign in <%= link_to 'here!', new_session_path %>